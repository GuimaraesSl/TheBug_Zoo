<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/drawer_menu"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/blue_fort"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    >

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageButton
            android:id="@+id/button_meio_umido"
            android:layout_width="339.5dp"
            android:layout_height="90dp"
            android:layout_marginTop="310dp"
            android:background="@null"
            app:srcCompat="@drawable/background_button"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints"/>

        <ImageButton
            android:id="@+id/button_taxidermizados"
            android:layout_width="339.5dp"
            android:layout_height="90dp"
            android:layout_marginTop="37dp"
            android:background="@drawable/background_button"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/button_meio_umido" />

        <ImageView
            android:id="@+id/icon_taxidermizados"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="24dp"
            app:layout_constraintEnd_toEndOf="@+id/button_taxidermizados"
            app:layout_constraintHorizontal_bias="0.089"
            app:layout_constraintStart_toStartOf="@+id/button_taxidermizados"
            app:layout_constraintTop_toTopOf="@+id/button_taxidermizados"
            app:srcCompat="@drawable/icon_taxidermizados" />

        <TextView
            android:id="@+id/text_taxidermizados"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:text="Taxidermizados"
            android:textColor="@color/white"
            android:textSize="21sp"
            app:layout_constraintEnd_toStartOf="@+id/icon_seta2"
            app:layout_constraintHorizontal_bias="0.425"
            app:layout_constraintStart_toEndOf="@+id/icon_taxidermizados"
            app:layout_constraintTop_toTopOf="@+id/button_taxidermizados" />

        <ImageView
            android:id="@+id/icon_seta2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="28dp"
            app:layout_constraintEnd_toEndOf="@+id/button_taxidermizados"
            app:layout_constraintTop_toTopOf="@+id/button_taxidermizados"
            app:srcCompat="@drawable/icon_seta" />

        <ImageButton
            android:id="@+id/button_osteologia"
            android:layout_width="339.5dp"
            android:layout_height="90dp"
            android:layout_marginTop="37dp"
            android:background="@drawable/background_button"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/button_taxidermizados" />

        <ImageView
            android:id="@+id/icon_meio_umido"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="24dp"
            app:layout_constraintEnd_toEndOf="@+id/button_meio_umido"
            app:layout_constraintHorizontal_bias="0.086"
            app:layout_constraintStart_toStartOf="@+id/button_meio_umido"
            app:layout_constraintTop_toTopOf="@+id/button_meio_umido"
            app:srcCompat="@drawable/icon_meio_umido" />

        <ImageView
            android:id="@+id/icon_seta"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="28dp"
            app:layout_constraintEnd_toEndOf="@+id/button_meio_umido"
            app:layout_constraintTop_toTopOf="@+id/button_meio_umido"
            app:srcCompat="@drawable/icon_seta" />

        <TextView
            android:id="@+id/text_meio_umido"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:text="Meio Ãšmido"
            android:textSize="21sp"
            android:textColor="@color/white"
            app:layout_constraintEnd_toStartOf="@+id/icon_seta"
            app:layout_constraintHorizontal_bias="0.478"
            app:layout_constraintStart_toEndOf="@+id/icon_meio_umido"
            app:layout_constraintTop_toTopOf="@+id/button_meio_umido" />

        <ImageView
            android:id="@+id/icon_osteologia"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="24dp"
            app:layout_constraintEnd_toEndOf="@+id/button_osteologia"
            app:layout_constraintHorizontal_bias="0.073"
            app:layout_constraintStart_toStartOf="@+id/button_osteologia"
            app:layout_constraintTop_toTopOf="@+id/button_osteologia"
            app:srcCompat="@drawable/icon_osteologia" />

        <ImageView
            android:id="@+id/icon_seta3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="28dp"
            app:layout_constraintEnd_toEndOf="@+id/button_osteologia"
            app:layout_constraintTop_toTopOf="@+id/button_osteologia"
            app:srcCompat="@drawable/icon_seta" />

        <TextView
            android:id="@+id/text_osteologia"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:text="Osteologia"
            android:textSize="21sp"
            android:textColor="@color/white"
            app:layout_constraintEnd_toStartOf="@+id/icon_seta3"
            app:layout_constraintHorizontal_bias="0.502"
            app:layout_constraintStart_toEndOf="@+id/icon_osteologia"
            app:layout_constraintTop_toTopOf="@+id/button_osteologia" />

        <ImageView
            android:id="@+id/background_circle"
            android:layout_width="515dp"
            android:layout_height="515dp"
            android:layout_marginBottom="420dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:srcCompat="@drawable/background_circle" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <include
        layout="@layout/drawer_toolbar"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
    <com.google.android.material.navigation.NavigationView
        android:id="@+id/navigation_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#000000FF"
        app:itemIconTint="#707070"
        app:itemIconSize="30dp"
        app:itemTextColor="#707070"
        app:itemTextAppearance="@style/DrawerStyle"
        app:menu="@menu/drawer_menu"
        app:headerLayout="@layout/drawer_header"
        android:layout_gravity="start">
    </com.google.android.material.navigation.NavigationView>


</androidx.drawerlayout.widget.DrawerLayout>